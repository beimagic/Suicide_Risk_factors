Rscript PRSice.R \
    --prsice PRSice_linux \
    --base /home1/beizhang/Suicide/PRS/BaseData/suicide_noUKB \
    --target /home1/beizhang/Suicide/PRS/QCed_UKB_genome \
    --thread 20 \
    --snp SNP \
    --A1 A1 \
    --A2 A2 \
    --chr CHR \
    --pvalue P \
    --bp BP \
    --stat OR \
    --no-regress \
    --score sum \
    --fastscore \
    --bar-levels 0.00001,0.0001,0.001,0.005,0.01,0.05,0.1,0.5,1 \
    --out UKB_SUI_QC_PRS_sum